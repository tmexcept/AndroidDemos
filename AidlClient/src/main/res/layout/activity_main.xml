<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.MainActivity">

    <Button
        android:id="@+id/bindClientService"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="bindClientService"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/testClientService"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="testClientService"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/bindClientService"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/bindRemoteService"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="bindRemoteService"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/testClientService"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/testRemoteService"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="testRemoteService"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/bindRemoteService"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>